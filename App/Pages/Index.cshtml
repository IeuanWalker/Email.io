                    "F11": function (cm) {
                        cm.setOption("fullScreen", !cm.getOption("fullScreen"));
                    },
                    "Esc": function (cm) {
                        if (cm.getOption("fullScreen")) cm.setOption("fullScreen", false);
                    }
                },
            });
            jsonEditor.on("change", function (jsonEditor, change) {
                UpdateResult();
            });

            resultEditor = CodeMirror.fromTextArea(document.getElementById("result-textarea"), {
                lineNumbers: true,
                matchBrackets: true,
                mode: "xml",
                htmlMode: true,
                autocorrect: true,
                lineWrapping: false,
                readOnly: true,
                extraKeys: {
                    "F11": function (cm) {
                        cm.setOption("fullScreen", !cm.getOption("fullScreen"));
                    },
                    "Esc": function (cm) {
                        if (cm.getOption("fullScreen")) cm.setOption("fullScreen", false);
                    }
                },
            });
            resultEditor.on("change", function (resultEditor, change) {
                UpdatePreview();
            });

            UpdateResult();
        });
        //when the bootstrap tab is fully shown, call codemirror's refresh().
        //Also, if you use jquery-UI, it's gonna be different here.
        $('.nav-tabs a').on('shown.bs.tab', function () {
            htmlEditor.refresh();
            jsonEditor.refresh();
            resultEditor.refresh();
        });

        function UpdateResult() {
            var template = Handlebars.compile(htmlEditor.getValue());
            var compiled = template(JSON.parse(jsonEditor.getValue()));
            resultEditor.getDoc().setValue(compiled);
        }

        function UpdatePreview() {
            document.getElementById("preview-iframe").src = "data:text/html;charset=utf-8," + escape(resultEditor.getValue());
        }


        var snippets = [
            { text: 'const', displayText: 'const declarations' },
            { text: 'let', displayText: 'let declarations' },
            { text: 'var', displayText: 'var declarations' }];

        function snippet() {
            CodeMirror.showHint(codemirror, function () {
                var cursor = codemirror.getCursor();
                var token = codemirror.getTokenAt(cursor);
                var start = token.start;
                var end = cursor.ch;
                var line = cursor.line;
                var currentWord = token.string;
                // 入力した文字列をスニペット配列から探す
                var list = snippets.filter(function (item) {
                    return item.text.indexOf(currentWord) >= 0;
                });
                return {
                    list: list.length ? list : snippets,
                    from: CodeMirror.Pos(line, start),
                    to: CodeMirror.Pos(line, end)
                };

            }, { completeSingle: false });
        }
