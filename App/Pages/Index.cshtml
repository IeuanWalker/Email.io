            cursor: 'row-resize',
        });

        var htmlEditor, jsonEditor, htmlEditor;
        $(document).ready(function () {
            htmlEditor = CodeMirror.fromTextArea(document.getElementById("html-textarea"), {
                lineNumbers: true,
                matchBrackets: true,
                mode: "htmlmixed",
                htmlMode: true,
                autocorrect: true,
                lineWrapping: false,
                extraKeys: {
                    "F11": function (cm) {
                        cm.setOption("fullScreen", !cm.getOption("fullScreen"));
                    },
                    "Esc": function (cm) {
                        if (cm.getOption("fullScreen")) cm.setOption("fullScreen", false);
                    },
                    "Ctrl-Space": "autocomplete"
                },
                hint: CodeMirror.hint.htmlHint
            });
            htmlEditor.on("change", function (htmlEditor, change) {
                UpdateResult();
            });

            jsonEditor = CodeMirror.fromTextArea(document.getElementById("json-textarea"), {
                lineNumbers: true,
                border: true,
                theme: "eclipse",
                mode: "application/json",
                gutters: ["CodeMirror-lint-markers"],
